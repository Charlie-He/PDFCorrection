{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport axios from 'axios';\nimport PdfUpload from './components/PdfUpload.vue';\nimport PdfButton from './components/PdfButton.vue';\nimport PdfProgress from './components/PdfProgress.vue';\nimport PdfModal from './components/PdfModal.vue';\nexport default {\n  name: 'App',\n  components: {\n    PdfUpload,\n    PdfButton,\n    PdfProgress,\n    PdfModal\n  },\n  data() {\n    return {\n      selectedFile: null,\n      isProcessing: false,\n      correctedFile: null,\n      errorMessage: '',\n      showPreviewModal: false,\n      compareMode: false,\n      previewTitle: '',\n      originalPdfUrl: null,\n      correctedPdfUrl: null,\n      previewPdfUrl: null,\n      detectedAngle: 0,\n      pageAngles: [],\n      processingTime: 0,\n      startTime: 0,\n      progressValue: 0,\n      progressInterval: null\n    };\n  },\n  methods: {\n    onFileSelect() {\n      this.errorMessage = '';\n    },\n    showPreview() {\n      if (!this.selectedFile) {\n        this.errorMessage = 'Êú™ÈÄâÊã©Êñá‰ª∂';\n        return;\n      }\n      this.compareMode = false;\n      this.previewTitle = 'ÂéüÂßãPDFÈ¢ÑËßà';\n      this.showPreviewModal = true;\n      this.previewPdfUrl = URL.createObjectURL(this.selectedFile);\n    },\n    async showCompareView() {\n      if (!this.selectedFile) {\n        this.errorMessage = 'Êú™ÈÄâÊã©Êñá‰ª∂';\n        return;\n      }\n      if (!this.correctedFile) {\n        this.errorMessage = 'ËØ∑ÂÖàÂ§ÑÁêÜÊñá‰ª∂ÂÜçËøõË°åÂØπÊØîÈ¢ÑËßà';\n        return;\n      }\n      this.compareMode = true;\n      this.previewTitle = 'Ê†°Ê≠£ÂâçÂêéÂØπÊØî';\n      this.showPreviewModal = true;\n      this.originalPdfUrl = URL.createObjectURL(this.selectedFile);\n      try {\n        const response = await axios.get(`http://localhost:8080/api/pdf/download/${this.correctedFile}`, {\n          responseType: 'blob'\n        });\n        this.correctedPdfUrl = URL.createObjectURL(response.data);\n      } catch (error) {\n        console.error('Âä†ËΩΩÊ†°Ê≠£Êñá‰ª∂Â§±Ë¥•:', error);\n        this.errorMessage = 'Âä†ËΩΩÊ†°Ê≠£Êñá‰ª∂Â§±Ë¥•: ' + (error.message || 'Êú™Áü•ÈîôËØØ');\n      }\n    },\n    closePreview() {\n      this.showPreviewModal = false;\n      if (this.originalPdfUrl) {\n        URL.revokeObjectURL(this.originalPdfUrl);\n        this.originalPdfUrl = null;\n      }\n      if (this.correctedPdfUrl) {\n        URL.revokeObjectURL(this.correctedPdfUrl);\n        this.correctedPdfUrl = null;\n      }\n      if (this.previewPdfUrl) {\n        URL.revokeObjectURL(this.previewPdfUrl);\n        this.previewPdfUrl = null;\n      }\n    },\n    async uploadAndCorrect() {\n      if (!this.selectedFile) {\n        this.errorMessage = 'Êú™ÈÄâÊã©Êñá‰ª∂';\n        return;\n      }\n      this.isProcessing = true;\n      this.errorMessage = '';\n      this.startTime = Date.now();\n      this.progressValue = 0;\n\n      // Ê®°ÊãüËøõÂ∫¶Êù°Â¢ûÈïø\n      this.progressInterval = setInterval(() => {\n        if (this.progressValue < 90) {\n          this.progressValue += 5;\n        }\n      }, 100);\n      const formData = new FormData();\n      formData.append('file', this.selectedFile);\n      try {\n        const response = await axios.post('http://localhost:8080/api/pdf/upload', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n\n        // Á°Æ‰øùËøõÂ∫¶Êù°ËææÂà∞100%\n        this.progressValue = 100;\n        if (this.progressInterval) {\n          clearInterval(this.progressInterval);\n          this.progressInterval = null;\n        }\n        if (response.data.success) {\n          this.correctedFile = response.data.fileName;\n          this.pageAngles = response.data.pageAngles || [];\n          if (this.pageAngles && this.pageAngles.length > 0) {\n            const sum = this.pageAngles.reduce((a, b) => a + b, 0);\n            this.detectedAngle = sum / this.pageAngles.length;\n          } else {\n            this.detectedAngle = response.data.angle || 0;\n          }\n          this.processingTime = (Date.now() - this.startTime) / 1000;\n        } else {\n          this.errorMessage = response.data.message || 'Â§ÑÁêÜÂ§±Ë¥•';\n        }\n      } catch (error) {\n        console.error('Êñá‰ª∂‰∏ä‰º†Â§±Ë¥•:', error);\n        if (this.progressInterval) {\n          clearInterval(this.progressInterval);\n          this.progressInterval = null;\n        }\n        if (error.response && error.response.data && error.response.data.message) {\n          this.errorMessage = error.response.data.message;\n        } else if (error.response) {\n          this.errorMessage = 'ÊúçÂä°Âô®Â§ÑÁêÜÂ§±Ë¥•: ' + (error.response.statusText || 'Êú™Áü•ÈîôËØØ');\n        } else if (error.request) {\n          this.errorMessage = 'Êó†Ê≥ïËøûÊé•Âà∞ÊúçÂä°Âô®ÔºåËØ∑Ê£ÄÊü•ÁΩëÁªúËøûÊé•ÊàñÊúçÂä°Âô®Áä∂ÊÄÅ';\n        } else {\n          this.errorMessage = '‰∏ä‰º†Â§±Ë¥•: ' + (error.message || 'Êú™Áü•ÈîôËØØ');\n        }\n      } finally {\n        this.isProcessing = false;\n        if (this.progressInterval) {\n          clearInterval(this.progressInterval);\n          this.progressInterval = null;\n          this.progressValue = 100;\n        }\n      }\n    },\n    async downloadFile() {\n      if (!this.correctedFile) return;\n      try {\n        const response = await axios.get(`http://localhost:8080/api/pdf/download/${this.correctedFile}`, {\n          responseType: 'blob'\n        });\n        const url = window.URL.createObjectURL(new Blob([response.data]));\n        const link = document.createElement('a');\n        link.href = url;\n        link.setAttribute('download', this.correctedFile);\n        document.body.appendChild(link);\n        link.click();\n        link.remove();\n        window.URL.revokeObjectURL(url);\n      } catch (error) {\n        console.error('‰∏ãËΩΩÂ§±Ë¥•:', error);\n        if (error.response && error.response.headers['error-message']) {\n          this.errorMessage = '‰∏ãËΩΩÂ§±Ë¥•: ' + error.response.headers['error-message'];\n        } else {\n          this.errorMessage = '‰∏ãËΩΩÂ§±Ë¥•ÔºåËØ∑ÈáçËØï';\n        }\n      }\n    },\n    reset() {\n      this.selectedFile = null;\n      this.correctedFile = null;\n      this.pageAngles = [];\n      this.errorMessage = '';\n      this.progressValue = 0;\n    }\n  },\n  beforeUnmount() {\n    if (this.progressInterval) {\n      clearInterval(this.progressInterval);\n    }\n  }\n};","map":{"version":3,"names":["axios","PdfUpload","PdfButton","PdfProgress","PdfModal","name","components","data","selectedFile","isProcessing","correctedFile","errorMessage","showPreviewModal","compareMode","previewTitle","originalPdfUrl","correctedPdfUrl","previewPdfUrl","detectedAngle","pageAngles","processingTime","startTime","progressValue","progressInterval","methods","onFileSelect","showPreview","URL","createObjectURL","showCompareView","response","get","responseType","error","console","message","closePreview","revokeObjectURL","uploadAndCorrect","Date","now","setInterval","formData","FormData","append","post","headers","clearInterval","success","fileName","length","sum","reduce","a","b","angle","statusText","request","downloadFile","url","window","Blob","link","document","createElement","href","setAttribute","body","appendChild","click","remove","reset","beforeUnmount"],"sources":["D:\\MyFile\\MyProject\\PDFCorrection\\frontend\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <div class=\"container\">\r\n      <h1 class=\"title\">üìÑ PDF È°µÈù¢ÂÄæÊñúÊ†°Ê≠£Á≥ªÁªü</h1>\r\n\r\n      <div class=\"upload-section\">\r\n        <PdfUpload\r\n          v-model=\"selectedFile\"\r\n          accept=\".pdf\"\r\n          :processing=\"isProcessing\"\r\n          @file-select=\"onFileSelect\"\r\n        />\r\n\r\n        <div v-if=\"selectedFile && !isProcessing && !correctedFile\" class=\"action-buttons\">\r\n          <PdfButton variant=\"outline-primary\" @click=\"showPreview\" fullWidth>\r\n            <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" width=\"20\" height=\"20\">\r\n              <path d=\"M1 12s4-8 11-8 11 8 11 8-4 8-11 8-11-8-11-8z\"></path>\r\n              <circle cx=\"12\" cy=\"12\" r=\"3\"></circle>\r\n            </svg>\r\n            È¢ÑËßàÂéüÊñá‰ª∂\r\n          </PdfButton>\r\n          <PdfButton variant=\"success\" @click=\"uploadAndCorrect\" :loading=\"isProcessing\" fullWidth>\r\n            ÂºÄÂßãÊ†°Ê≠£\r\n          </PdfButton>\r\n        </div>\r\n      </div>\r\n\r\n      <PdfProgress\r\n        :show=\"isProcessing\"\r\n        type=\"bar\"\r\n        :progress=\"progressValue\"\r\n        message=\"Ê≠£Âú®Â§ÑÁêÜ‰∏≠ÔºåËØ∑Á®çÂÄô...\"\r\n      />\r\n\r\n      <div v-if=\"errorMessage\" class=\"error-message\">\r\n        <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\">\r\n          <circle cx=\"12\" cy=\"12\" r=\"10\"></circle>\r\n          <line x1=\"12\" y1=\"8\" x2=\"12\" y2=\"12\"></line>\r\n          <line x1=\"12\" y1=\"16\" x2=\"12.01\" y2=\"16\"></line>\r\n        </svg>\r\n        {{ errorMessage }}\r\n      </div>\r\n\r\n      <div v-if=\"correctedFile\" class=\"success-section\">\r\n        <div class=\"success-message\">\r\n          <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\">\r\n            <path d=\"M22 11.08V12a10 10 0 1 1-5.93-9.14\"></path>\r\n            <polyline points=\"22 4 12 14.01 9 11.01\"></polyline>\r\n          </svg>\r\n          <span>Ê†°Ê≠£ÂÆåÊàêÔºÅ</span>\r\n        </div>\r\n        \r\n        <div class=\"correction-info\">\r\n          <p>Â§ÑÁêÜËÄóÊó∂: {{ processingTime.toFixed(2) }}Áßí</p>\r\n        </div>\r\n\r\n        <div class=\"action-buttons\">\r\n          <PdfButton variant=\"outline-primary\" @click=\"showCompareView\" fullWidth>\r\n            <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" width=\"20\" height=\"20\">\r\n              <rect x=\"3\" y=\"3\" width=\"7\" height=\"7\"></rect>\r\n              <rect x=\"14\" y=\"3\" width=\"7\" height=\"7\"></rect>\r\n              <rect x=\"14\" y=\"14\" width=\"7\" height=\"7\"></rect>\r\n              <rect x=\"3\" y=\"14\" width=\"7\" height=\"7\"></rect>\r\n            </svg>\r\n            ÂØπÊØîÈ¢ÑËßà\r\n          </PdfButton>\r\n          <PdfButton variant=\"primary\" @click=\"downloadFile\" fullWidth>\r\n            <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" width=\"20\" height=\"20\">\r\n              <path d=\"M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4\"></path>\r\n              <polyline points=\"7 10 12 15 17 10\"></polyline>\r\n              <line x1=\"12\" y1=\"15\" x2=\"12\" y2=\"3\"></line>\r\n            </svg>\r\n            ‰∏ãËΩΩÊ†°Ê≠£ÂêéÁöÑPDF\r\n          </PdfButton>\r\n        </div>\r\n\r\n        <PdfButton variant=\"secondary\" @click=\"reset\" fullWidth>\r\n          Â§ÑÁêÜÊñ∞Êñá‰ª∂\r\n        </PdfButton>\r\n      </div>\r\n    </div>\r\n\r\n    <PdfModal v-model:visible=\"showPreviewModal\" :title=\"previewTitle\" content-class=\"preview-modal\">\r\n      <div v-if=\"compareMode\" class=\"compare-container\">\r\n        <div class=\"preview-panel\">\r\n          <h3>ÂéüÂßãÊñá‰ª∂</h3>\r\n          <div class=\"pdf-viewer\">\r\n            <iframe :src=\"originalPdfUrl\" v-if=\"originalPdfUrl\"></iframe>\r\n            <div v-else>Âä†ËΩΩ‰∏≠...</div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"preview-panel\">\r\n          <h3>Ê†°Ê≠£ÂêéÊñá‰ª∂</h3>\r\n          <div class=\"pdf-viewer\">\r\n            <iframe :src=\"correctedPdfUrl\" v-if=\"correctedPdfUrl\"></iframe>\r\n            <div v-else>Âä†ËΩΩ‰∏≠...</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-else class=\"single-preview\">\r\n        <div class=\"pdf-viewer\">\r\n          <iframe :src=\"previewPdfUrl\" v-if=\"previewPdfUrl\"></iframe>\r\n          <div v-else>Âä†ËΩΩ‰∏≠...</div>\r\n        </div>\r\n      </div>\r\n    </PdfModal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport PdfUpload from './components/PdfUpload.vue';\r\nimport PdfButton from './components/PdfButton.vue';\r\nimport PdfProgress from './components/PdfProgress.vue';\r\nimport PdfModal from './components/PdfModal.vue';\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    PdfUpload,\r\n    PdfButton,\r\n    PdfProgress,\r\n    PdfModal\r\n  },\r\n  data() {\r\n    return {\r\n      selectedFile: null,\r\n      isProcessing: false,\r\n      correctedFile: null,\r\n      errorMessage: '',\r\n      showPreviewModal: false,\r\n      compareMode: false,\r\n      previewTitle: '',\r\n      originalPdfUrl: null,\r\n      correctedPdfUrl: null,\r\n      previewPdfUrl: null,\r\n      detectedAngle: 0,\r\n      pageAngles: [],\r\n      processingTime: 0,\r\n      startTime: 0,\r\n      progressValue: 0,\r\n      progressInterval: null\r\n    };\r\n  },\r\n  methods: {\r\n    onFileSelect() {\r\n      this.errorMessage = '';\r\n    },\r\n\r\n    showPreview() {\r\n      if (!this.selectedFile) {\r\n        this.errorMessage = 'Êú™ÈÄâÊã©Êñá‰ª∂';\r\n        return;\r\n      }\r\n\r\n      this.compareMode = false;\r\n      this.previewTitle = 'ÂéüÂßãPDFÈ¢ÑËßà';\r\n      this.showPreviewModal = true;\r\n\r\n      this.previewPdfUrl = URL.createObjectURL(this.selectedFile);\r\n    },\r\n\r\n    async showCompareView() {\r\n      if (!this.selectedFile) {\r\n        this.errorMessage = 'Êú™ÈÄâÊã©Êñá‰ª∂';\r\n        return;\r\n      }\r\n\r\n      if (!this.correctedFile) {\r\n        this.errorMessage = 'ËØ∑ÂÖàÂ§ÑÁêÜÊñá‰ª∂ÂÜçËøõË°åÂØπÊØîÈ¢ÑËßà';\r\n        return;\r\n      }\r\n\r\n      this.compareMode = true;\r\n      this.previewTitle = 'Ê†°Ê≠£ÂâçÂêéÂØπÊØî';\r\n      this.showPreviewModal = true;\r\n\r\n      this.originalPdfUrl = URL.createObjectURL(this.selectedFile);\r\n\r\n      try {\r\n        const response = await axios.get(\r\n          `http://localhost:8080/api/pdf/download/${this.correctedFile}`,\r\n          { responseType: 'blob' }\r\n        );\r\n        \r\n        this.correctedPdfUrl = URL.createObjectURL(response.data);\r\n      } catch (error) {\r\n        console.error('Âä†ËΩΩÊ†°Ê≠£Êñá‰ª∂Â§±Ë¥•:', error);\r\n        this.errorMessage = 'Âä†ËΩΩÊ†°Ê≠£Êñá‰ª∂Â§±Ë¥•: ' + (error.message || 'Êú™Áü•ÈîôËØØ');\r\n      }\r\n    },\r\n\r\n    closePreview() {\r\n      this.showPreviewModal = false;\r\n      \r\n      if (this.originalPdfUrl) {\r\n        URL.revokeObjectURL(this.originalPdfUrl);\r\n        this.originalPdfUrl = null;\r\n      }\r\n      \r\n      if (this.correctedPdfUrl) {\r\n        URL.revokeObjectURL(this.correctedPdfUrl);\r\n        this.correctedPdfUrl = null;\r\n      }\r\n      \r\n      if (this.previewPdfUrl) {\r\n        URL.revokeObjectURL(this.previewPdfUrl);\r\n        this.previewPdfUrl = null;\r\n      }\r\n    },\r\n\r\n    async uploadAndCorrect() {\r\n      if (!this.selectedFile) {\r\n        this.errorMessage = 'Êú™ÈÄâÊã©Êñá‰ª∂';\r\n        return;\r\n      }\r\n\r\n      this.isProcessing = true;\r\n      this.errorMessage = '';\r\n      this.startTime = Date.now();\r\n      this.progressValue = 0;\r\n      \r\n      // Ê®°ÊãüËøõÂ∫¶Êù°Â¢ûÈïø\r\n      this.progressInterval = setInterval(() => {\r\n        if (this.progressValue < 90) {\r\n          this.progressValue += 5;\r\n        }\r\n      }, 100);\r\n\r\n      const formData = new FormData();\r\n      formData.append('file', this.selectedFile);\r\n\r\n      try {\r\n        const response = await axios.post('http://localhost:8080/api/pdf/upload', formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n        });\r\n\r\n        // Á°Æ‰øùËøõÂ∫¶Êù°ËææÂà∞100%\r\n        this.progressValue 