{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport PdfUpload from './components/PdfUpload.vue';\nimport PdfProgress from './components/PdfProgress.vue';\nimport PdfButton from './components/PdfButton.vue';\nimport PdfModal from './components/PdfModal.vue';\nimport axios from 'axios';\nexport default {\n  name: 'App',\n  components: {\n    PdfUpload,\n    PdfProgress,\n    PdfButton,\n    PdfModal\n  },\n  data() {\n    return {\n      selectedFile: null,\n      correctedFile: null,\n      pageAngles: [],\n      isProcessing: false,\n      progressValue: 0,\n      progressInterval: null,\n      processSteps: [],\n      errorMessage: '',\n      detectedAngle: null,\n      showPreview: false,\n      eventSource: null,\n      startTime: 0,\n      processingTime: 0,\n      currentStep: '准备处理...',\n      taskId: null,\n      // 添加任务ID\n      statusCheckInterval: null // 添加状态检查定时器\n    };\n  },\n  computed: {\n    originalFileUrl() {\n      return this.selectedFile ? URL.createObjectURL(this.selectedFile) : '';\n    },\n    correctedFileUrl() {\n      return this.correctedFile ? `http://localhost:8080/api/pdf/download/${this.correctedFile}` : '';\n    }\n  },\n  methods: {\n    handleFileSelect(file) {\n      this.selectedFile = file;\n      this.errorMessage = '';\n    },\n    formatFileSize(bytes) {\n      if (bytes === 0) return '0 Bytes';\n      const k = 1024;\n      const sizes = ['Bytes', 'KB', 'MB', 'GB'];\n      const i = Math.floor(Math.log(bytes) / Math.log(k));\n      return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];\n    },\n    addProcessStep(message) {\n      // 检查是否是重复的消息\n      const isDuplicate = this.processSteps.some(step => step.message === message);\n      if (isDuplicate) {\n        return; // 如果是重复消息，则不添加\n      }\n      this.processSteps.push({\n        message: message,\n        completed: true,\n        current: false\n      });\n    },\n    // 更新当前处理步骤\n    updateCurrentStep(message) {\n      // 检查是否是重复的消息\n      const isDuplicate = this.processSteps.some(step => step.message === message);\n      if (isDuplicate) {\n        return; // 如果是重复消息，则不添加\n      }\n\n      // 将之前的步骤标记为完成\n      this.processSteps.forEach(step => {\n        step.current = false;\n        step.completed = true;\n      });\n\n      // 添加新的当前步骤\n      this.processSteps.push({\n        message: message,\n        completed: false,\n        current: true\n      });\n    },\n    async uploadAndCorrect() {\n      if (!this.selectedFile) {\n        this.errorMessage = '未选择文件';\n        return;\n      }\n      this.isProcessing = true;\n      this.errorMessage = '';\n      this.startTime = Date.now();\n      this.progressValue = 0;\n      this.processSteps = []; // 清空之前的步骤\n      this.batchDetails = []; // 清空批次详情\n      this.detectedAngle = null; // 重置角度显示\n\n      // 建立SSE连接以接收实时进度更新\n      this.eventSource = new EventSource('http://localhost:8080/api/pdf/progress');\n      this.eventSource.addEventListener('progress', event => {\n        this.addProcessStep(event.data);\n      });\n      this.eventSource.addEventListener('angle', event => {\n        this.detectedAngle = parseFloat(event.data);\n      });\n      this.eventSource.onerror = error => {\n        console.error('SSE连接错误:', error);\n      };\n\n      // 模拟进度条增长\n      this.progressInterval = setInterval(() => {\n        if (this.progressValue < 90) {\n          this.progressValue += 5;\n        }\n      }, 100);\n\n      // 显示开始处理信息\n      this.updateCurrentStep('开始处理PDF文件...');\n      const formData = new FormData();\n      formData.append('file', this.selectedFile);\n      try {\n        const response = await axios.post('http://localhost:8080/api/pdf/upload', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          },\n          onUploadProgress: progressEvent => {\n            if (progressEvent.lengthComputable) {\n              const percentCompleted = Math.round(progressEvent.loaded * 100 / progressEvent.total);\n              this.progressValue = Math.min(percentCompleted, 90);\n            }\n          }\n        });\n\n        // 确保进度条达到100%\n        this.progressValue = 100;\n        if (this.progressInterval) {\n          clearInterval(this.progressInterval);\n          this.progressInterval = null;\n        }\n        if (response.data.success) {\n          // 获取任务ID\n          this.taskId = response.data.fileName;\n\n          // 开始轮询任务状态\n          this.statusCheckInterval = setInterval(() => {\n            this.checkTaskStatus();\n          }, 1000);\n        } else {\n          this.errorMessage = response.data.message || '处理失败';\n        }\n      } catch (error) {\n        console.error('文件上传失败:', error);\n        if (this.progressInterval) {\n          clearInterval(this.progressInterval);\n          this.progressInterval = null;\n        }\n        if (error.response && error.response.data && error.response.data.message) {\n          this.errorMessage = error.response.data.message;\n        } else if (error.response) {\n          this.errorMessage = '服务器处理失败: ' + (error.response.statusText || '未知错误');\n        } else if (error.request) {\n          this.errorMessage = '无法连接到服务器，请检查网络连接或服务器状态';\n        } else {\n          this.errorMessage = '上传失败: ' + (error.message || '未知错误');\n        }\n      } finally {\n        this.isProcessing = false;\n        if (this.progressInterval) {\n          clearInterval(this.progressInterval);\n          this.progressInterval = null;\n          this.progressValue = 100;\n        }\n\n        // 关闭SSE连接\n        if (this.eventSource) {\n          this.eventSource.close();\n          this.eventSource = null;\n        }\n      }\n    },\n    // 检查任务状态\n    async checkTaskStatus() {\n      if (!this.taskId) return;\n      try {\n        const response = await axios.get(`http://localhost:8080/api/pdf/status/${this.taskId}`);\n        if (response.data.success) {\n          if (response.data.fileName && response.data.fileName !== this.taskId) {\n            // 处理完成\n            clearInterval(this.statusCheckInterval);\n            this.statusCheckInterval = null;\n            this.correctedFile = response.data.fileName;\n            this.pageAngles = response.data.pageAngles || [];\n            if (this.pageAngles && this.pageAngles.length > 0) {\n              const sum = this.pageAngles.reduce((a, b) => a + b, 0);\n              this.detectedAngle = sum / this.pageAngles.length;\n            } else {\n              this.detectedAngle = response.data.angle || 0;\n            }\n            this.processingTime = (Date.now() - this.startTime) / 1000;\n\n            // 完成所有步骤\n            this.processSteps.forEach(step => {\n              step.current = false;\n              step.completed = true;\n            });\n          }\n        } else {\n          // 处理失败\n          clearInterval(this.statusCheckInterval);\n          this.statusCheckInterval = null;\n          this.errorMessage = response.data.message || '处理失败';\n        }\n      } catch (error) {\n        console.error('检查任务状态失败:', error);\n      }\n    },\n    async downloadFile() {\n      if (!this.correctedFile) return;\n      try {\n        const response = await axios.get(`http://localhost:8080/api/pdf/download/${this.correctedFile}`, {\n          responseType: 'blob'\n        });\n        const url = window.URL.createObjectURL(new Blob([response.data]));\n        const link = document.createElement('a');\n        link.href = url;\n        link.setAttribute('download', this.correctedFile);\n        document.body.appendChild(link);\n        link.click();\n        link.remove();\n        window.URL.revokeObjectURL(url);\n      } catch (error) {\n        console.error('下载失败:', error);\n        if (error.response && error.response.headers['error-message']) {\n          this.errorMessage = '下载失败: ' + error.response.headers['error-message'];\n        } else {\n          this.errorMessage = '下载失败，请重试';\n        }\n      }\n    },\n    previewFile() {\n      this.showPreview = true;\n    },\n    reset() {\n      this.selectedFile = null;\n      this.correctedFile = null;\n      this.pageAngles = [];\n      this.errorMessage = '';\n      this.progressValue = 0;\n      this.detectedAngle = null;\n      this.processSteps = [];\n      this.currentStep = '准备处理...';\n      this.taskId = null;\n      if (this.statusCheckInterval) {\n        clearInterval(this.statusCheckInterval);\n        this.statusCheckInterval = null;\n      }\n    }\n  },\n  beforeUnmount() {\n    // 清理定时器\n    if (this.progressInterval) {\n      clearInterval(this.progressInterval);\n    }\n    if (this.statusCheckInterval) {\n      clearInterval(this.statusCheckInterval);\n    }\n\n    // 关闭SSE连接\n    if (this.eventSource) {\n      this.eventSource.close();\n    }\n  }\n};","map":{"version":3,"names":["PdfUpload","PdfProgress","PdfButton","PdfModal","axios","name","components","data","selectedFile","correctedFile","pageAngles","isProcessing","progressValue","progressInterval","processSteps","errorMessage","detectedAngle","showPreview","eventSource","startTime","processingTime","currentStep","taskId","statusCheckInterval","computed","originalFileUrl","URL","createObjectURL","correctedFileUrl","methods","handleFileSelect","file","formatFileSize","bytes","k","sizes","i","Math","floor","log","parseFloat","pow","toFixed","addProcessStep","message","isDuplicate","some","step","push","completed","current","updateCurrentStep","forEach","uploadAndCorrect","Date","now","batchDetails","EventSource","addEventListener","event","onerror","error","console","setInterval","formData","FormData","append","response","post","headers","onUploadProgress","progressEvent","lengthComputable","percentCompleted","round","loaded","total","min","clearInterval","success","fileName","checkTaskStatus","statusText","request","close","get","length","sum","reduce","a","b","angle","downloadFile","responseType","url","window","Blob","link","document","createElement","href","setAttribute","body","appendChild","click","remove","revokeObjectURL","previewFile","reset","beforeUnmount"],"sources":["D:\\MyFile\\MyProject\\PDFCorrection\\frontend\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <header class=\"header\">\r\n      <h1>PDF页面倾斜校正系统</h1>\r\n      <p>自动检测并校正PDF文档中的页面倾斜问题</p>\r\n    </header>\r\n\r\n    <main class=\"main-content\">\r\n      <div class=\"upload-section\">\r\n        <PdfUpload \r\n          v-model=\"selectedFile\" \r\n          @file-select=\"handleFileSelect\"\r\n          :disabled=\"isProcessing\"\r\n          :processing=\"isProcessing\"\r\n        />\r\n      </div>\r\n\r\n      <div v-if=\"selectedFile && !correctedFile\" class=\"file-info\">\r\n        <h3>选中文件:</h3>\r\n        <p>{{ selectedFile.name }} ({{ formatFileSize(selectedFile.size) }})</p>\r\n        <PdfButton @click=\"uploadAndCorrect\" :disabled=\"isProcessing\">\r\n          {{ isProcessing ? '处理中...' : '开始校正' }}\r\n        </PdfButton>\r\n      </div>\r\n\r\n      <div v-if=\"isProcessing || progressValue > 0\" class=\"progress-section\">\r\n        <PdfProgress \r\n          :type=\"'bar'\"\r\n          :message=\"currentStep\"\r\n          :progress=\"progressValue\"\r\n        />\r\n        \r\n        <div class=\"process-steps\" v-if=\"processSteps.length > 0\">\r\n          <h3>处理进度:</h3>\r\n          <ul>\r\n            <li \r\n              v-for=\"(step, index) in processSteps\" \r\n              :key=\"index\" \r\n              :class=\"{ 'completed': step.completed, 'current': step.current }\"\r\n            >\r\n              {{ step.message }}\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-if=\"errorMessage\" class=\"error-message\">\r\n        <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\">\r\n          <circle cx=\"12\" cy=\"12\" r=\"10\"></circle>\r\n          <line x1=\"12\" y1=\"8\" x2=\"12\" y2=\"12\"></line>\r\n          <line x1=\"12\" y1=\"16\" x2=\"12.01\" y2=\"16\"></line>\r\n        </svg>\r\n        {{ errorMessage }}\r\n      </div>\r\n\r\n      <div v-if=\"correctedFile\" class=\"success-section\">\r\n        <div class=\"success-message\">\r\n          <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\">\r\n            <path d=\"M22 11.08V12a10 10 0 1 1-5.93-9.14\"></path>\r\n            <polyline points=\"22 4 12 14.01 9 11.01\"></polyline>\r\n          </svg>\r\n          <span>校正完成！</span>\r\n        </div>\r\n        \r\n        <div class=\"correction-info\" v-if=\"processingTime > 0\">\r\n          <div>\r\n            <div>\r\n              <p>各页面的倾斜角度详情:</p>\r\n              <ul>\r\n                <li v-for=\"(angle, index) in pageAngles\" :key=\"index\">\r\n                  第{{ index + 1 }}页: {{ angle.toFixed(2) }}°\r\n                </li>\r\n              </ul>\r\n            </div>\r\n            <p>处理耗时: {{ processingTime.toFixed(2) }}秒</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"action-buttons\">\r\n          <PdfButton @click=\"downloadFile\" :disabled=\"!correctedFile\">\r\n            下载校正后的PDF\r\n          </PdfButton>\r\n          <PdfButton @click=\"previewFile\" :disabled=\"!correctedFile\" class=\"preview-button\">\r\n            预览校正效果\r\n          </PdfButton>\r\n          <PdfButton @click=\"reset\" class=\"reset-button\">\r\n            重新上传\r\n          </PdfButton>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-if=\"detectedAngle !== null\" class=\"angle-display\">\r\n        <p>检测到的平均倾斜角度: {{ detectedAngle.toFixed(2) }}°</p>\r\n      </div>\r\n    </main>\r\n\r\n    <PdfModal :visible=\"showPreview\" @close=\"showPreview = false\" title=\"校正效果预览\">\r\n      <div class=\"preview-modal\">\r\n        <div class=\"compare-container\" v-if=\"selectedFile && correctedFile\">\r\n          <div class=\"preview-panel\">\r\n            <h3>原始文件</h3>\r\n            <div class=\"pdf-viewer single-preview\">\r\n              <iframe :src=\"originalFileUrl\" frameborder=\"0\"></iframe>\r\n            </div>\r\n          </div>\r\n          <div class=\"preview-panel\">\r\n            <h3>校正后文件</h3>\r\n            <div class=\"pdf-viewer single-preview\">\r\n              <iframe :src=\"correctedFileUrl\" frameborder=\"0\"></iframe>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"pdf-viewer\" v-else-if=\"correctedFile\">\r\n          <iframe :src=\"correctedFileUrl\" frameborder=\"0\"></iframe>\r\n        </div>\r\n      </div>\r\n    </PdfModal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PdfUpload from './components/PdfUpload.vue';\r\nimport PdfProgress from './components/PdfProgress.vue';\r\nimport PdfButton from './components/PdfButton.vue';\r\nimport PdfModal from './components/PdfModal.vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    PdfUpload,\r\n    PdfProgress,\r\n    PdfButton,\r\n    PdfModal\r\n  },\r\n  data() {\r\n    return {\r\n      selectedFile: null,\r\n      correctedFile: null,\r\n      pageAngles: [],\r\n      isProcessing: false,\r\n      progressValue: 0,\r\n      progressInterval: null,\r\n      processSteps: [],\r\n      errorMessage: '',\r\n      detectedAngle: null,\r\n      showPreview: false,\r\n      eventSource: null,\r\n      startTime: 0,\r\n      processingTime: 0,\r\n      currentStep: '准备处理...',\r\n      taskId: null, // 添加任务ID\r\n      statusCheckInterval: null // 添加状态检查定时器\r\n    };\r\n  },\r\n  computed: {\r\n    originalFileUrl() {\r\n      return this.selectedFile ? URL.createObjectURL(this.selectedFile) : '';\r\n    },\r\n    correctedFileUrl() {\r\n      return this.correctedFile ? `http://localhost:8080/api/pdf/download/${this.correctedFile}` : '';\r\n    }\r\n  },\r\n  methods: {\r\n    handleFileSelect(file) {\r\n      this.selectedFile = file;\r\n      this.errorMessage = '';\r\n    },\r\n\r\n    formatFileSize(bytes) {\r\n      if (bytes === 0) return '0 Bytes';\r\n      const k = 1024;\r\n      const sizes = ['Bytes', 'KB', 'MB', 'GB'];\r\n      const i = Math.floor(Math.log(bytes) / Math.log(k));\r\n      return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];\r\n    },\r\n\r\n    addProcessStep(message) {\r\n      // 检查是否是重复的消息\r\n      const isDuplicate = this.processSteps.some(step => step.message === message);\r\n      if (isDuplicate) {\r\n        return; // 如果是重复消息，则不添加\r\n      }\r\n      \r\n      this.processSteps.push({\r\n        message: message,\r\n        completed: true,\r\n        current: false\r\n      });\r\n    },\r\n\r\n    // 更新当前处理步骤\r\n    updateCurrentStep(message) {\r\n      // 检查是否是重复的消息\r\n      const isDuplicate = this.processSteps.some(step => step.message === message);\r\n      if (isDuplicate) {\r\n        return; // 如果是重复消息，则不添加\r\n      }\r\n      \r\n      // 将之前的步骤标记为完成\r\n      this.processSteps.forEach(step => {\r\n        step.current = false;\r\n        step.completed = true;\r\n      });\r\n      \r\n      // 添加新的当前步骤\r\n      this.processSteps.push({\r\n        message: message,\r\n        completed: false,\r\n        current: true\r\n      });\r\n    },\r\n\r\n    async uploadAndCorrect() {\r\n      if (!this.selectedFile) {\r\n        this.errorMessage = '未选择文件';\r\n        return;\r\n      }\r\n\r\n      this.isProcessing = true;\r\n      this.errorMessage = '';\r\n      this.startTime = Date.now();\r\n      this.progressValue = 0;\r\n      this.processSteps = []; // 清空之前的步骤\r\n      this.batchDetails = []; // 清空批次详情\r\n      this.detectedAngle = null; // 重置角度显示\r\n      \r\n      // 建立SSE连接以接收实时进度更新\r\n      this.eventSource = new EventSource('http://localhost:8080/api/pdf/progress');\r\n      \r\n      this.eventSource.addEventListener('progress', (event) => {\r\n        this.addProcessStep(event.data);\r\n      });\r\n      \r\n      this.eventSource.addEventListener('angle', (event) => {\r\n        this.detectedAngle = parseFloat(event.data);\r\n      });\r\n      \r\n      this.eventSource.onerror = (error) => {\r\n        console.error('SSE连接错误:', error);\r\n      };\r\n\r\n      // 模拟进度条增长\r\n      this.progressInterval = setInterval(() => {\r\n        if (this.progressValue < 90) {\r\n          this.progressValue += 5;\r\n        }\r\n      }, 100);\r\n\r\n      // 显示开始处理信息\r\n      this.updateCurrentStep('开始处理PDF文件...');\r\n\r\n      const formData = new FormData();\r\n      formData.append('file', this.selectedFile);\r\n\r\n      try {\r\n        const response = await axios.post('http://localhost:8080/api/pdf/upload', formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          },\r\n          onUploadProgress: (progressEvent) => {\r\n            if (progressEvent.lengthComputable) {\r\n              const percentCompleted = Math.round((progressEvent.loaded * 100) / progressEvent.total);\r\n              this.progressValue = Math.min(percentCompleted, 90);\r\n            }\r\n          }\r\n        });\r\n\r\n        // 确保进度条达到100%\r\n        this.progressValue = 100;\r\n        \r\n        if (this.progressInterval) {\r\n          clearInterval(this.progressInterval);\r\n          this.progressInterval = null;\r\n        }\r\n\r\n        if (response.data.success) {\r\n          // 获取任务ID\r\n          this.taskId = response.data.fileName;\r\n          \r\n          // 开始轮询任务状态\r\n          this.statusCheckInterval = setInterval(() => {\r\n            this.checkTaskStatus();\r\n          }, 1000);\r\n        } else {\r\n          th